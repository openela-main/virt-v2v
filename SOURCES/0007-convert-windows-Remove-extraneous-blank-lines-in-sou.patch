         (sprintf "install-%s.ps1" msi_path) !psh_script;
     ) files
 
-
   and unconfigure_xenpv () =
     match xenpv_uninst with
     | None -> () (* nothing to be uninstalled *)
diff --git a/convert/windows_virtio.ml b/convert/windows_virtio.ml
